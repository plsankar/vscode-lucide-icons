{
  "name": "vscode-lucide-icons",
  "displayName": "Lucide Icons",
  "description": "Add Lucide icons to your project",
  "version": "0.0.3",
  "publisher": "Lakshmisankar",
  "icon": "images/vscode-lucide-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/plsankar/vscode-lucide-icons.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-lucide-icons.browse",
        "title": "Lucide Icons: Browse Icons Library"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "~22.13.10",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.22.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "lucide-static": "^0.479.0"
  },
  "packageManager": "pnpm@10.6.3"
}